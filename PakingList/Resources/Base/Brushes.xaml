<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:p="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">

    <SolidColorBrush
        x:Key="TransparentBrush"
        p:Freeze="true"
        Color="Transparent" />
    <SolidColorBrush
        x:Key="BlackBrush"
        p:Freeze="true"
        Color="Black" />
    <SolidColorBrush
        x:Key="WhiteBrush"
        p:Freeze="true"
        Color="White" />

    <SolidColorBrush
        x:Key="RedBrush"
        p:Freeze="true"
        Color="Red" />
    <SolidColorBrush
        x:Key="GreenBrush"
        p:Freeze="true"
        Color="Green" />
    <SolidColorBrush
        x:Key="BlueBrush"
        p:Freeze="true"
        Color="Blue" />

    <SolidColorBrush
        x:Key="LightGrayBrush"
        p:Freeze="true"
        Color="LightGray" />
    <SolidColorBrush
        x:Key="GrayBrush"
        p:Freeze="true"
        Color="Gray" />
    <SolidColorBrush
        x:Key="DarkGrayBrush"
        p:Freeze="true"
        Color="DarkGray" />

    <SolidColorBrush
        x:Key="OrangeBrush"
        p:Freeze="true"
        Color="Orange" />
    <SolidColorBrush
        x:Key="OrangeRedBrush"
        p:Freeze="true"
        Color="OrangeRed" />
    <SolidColorBrush
        x:Key="DarkBrush"
        p:Freeze="true"
        Color="DarkRed" />

</ResourceDictionary>